5\n-- SELECT DISTINCT pc1.model, pc2.model\n-- FROM pc AS pc1, pc AS pc2\n-- WHERE pc1.model > pc2.model AND pc1.speed = pc2.speed AND pc1.ram = pc2.ram\n\n-- exercicio 16\n-- SELECT DISTINCT product.maker\n-- FROM product WHERE product.model IN\n-- (\n--   SELECT model FROM pc\n--   WHERE speed >= 133\n--\n--   UNION\n--\n--   SELECT model FROM laptop\n--   WHERE speed >= 133\n-- );\n\n-- exercicio 17\n-- SELECT DISTINCT product.maker\n-- FROM product WHERE product.model IN\n-- (\n--   SELECT model, MAX(speed) as s FROM pc GROUP BY model\n--\n--   UNION\n--\n--   SELECT model, MAX(speed) FROM laptop\n--   GROUP BY model\n-- );\n\n-- SELECT pc1.*\n-- FROM pc as pc1\n-- LEFT OUTER JOIN pc as pc2\n--     ON pc1.model = pc2.model AND pc1.speed < pc2.speed\n-- WHERE pc2.model IS NULL;\n\nselect model, MAX(speed)\nfrom pc\nwhere model = (\n    select max(speed)\n    from pc\n)", error: GroupingError('column "pc.model" must appear in the GROUP BY clause or be used in an aggregate function\nLINE 109: select model, MAX(speed)\n                 ^\n')
