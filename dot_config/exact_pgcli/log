5\n-- SELECT DISTINCT pc1.model, pc2.model\n-- FROM pc AS pc1, pc AS pc2\n-- WHERE pc1.model > pc2.model AND pc1.speed = pc2.speed AND pc1.ram = pc2.ram\n\n-- exercicio 16\n-- SELECT DISTINCT product.maker\n-- FROM product WHERE product.model IN\n-- (\n--   SELECT model FROM pc\n--   WHERE speed >= 133\n--\n--   UNION\n--\n--   SELECT model FROM laptop\n--   WHERE speed >= 133\n-- );\n\n-- exercicio 17\n-- SELECT DISTINCT product.maker\n-- FROM product WHERE product.model IN\n-- (\n--   select model\n--   from pc\n--   where speed = (\n--       select max(speed)\n--       from pc\n--   )\n--\n--   UNION\n--\n--   select model\n--   from laptop\n--   where speed = (\n--       select max(speed)\n--       from laptop\n--   )\n-- );\n\n-- SELECT DISTINCT product.makerSELECT model from product inner JOIN (\n-- SELECT *\n-- from product inner JOIN\n-- from product\nSELECT DISTINCT maker\nFROM product\nWHERE product.model IN (\n  SELECT model from (\n    select speed, model\n    from pc\n    where speed >= ALL (\n        select speed\n        from pc\n    )\n\n    UNION\n\n    select model\n    from laptop\n    where speed >= ALL (\n      select speed\n      from laptop\n    )\n  )\n) as computers", error: SyntaxError('subquery in FROM must have an alias\nLINE 119:   SELECT model from (\n                              ^\nHINT:  For example, FROM (SELECT ...) [AS] foo.\n')
