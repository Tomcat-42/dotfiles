15\n-- SELECT DISTINCT pc1.model, pc2.model\n-- FROM pc AS pc1, pc AS pc2\n-- WHERE pc1.model > pc2.model AND pc1.speed = pc2.speed AND pc1.ram = pc2.ram\n\n-- exercicio 16\n-- SELECT DISTINCT product.maker\n-- FROM product WHERE product.model IN\n-- (\n--   SELECT model FROM pc\n--   WHERE speed >= 133\n--\n--   UNION\n--\n--   SELECT model FROM laptop\n--   WHERE speed >= 133\n-- );\n\n-- exercicio 17\n-- CREATE TEMP TABLE model_speed (model INT, speed FLOAT);\n--\n-- INSERT INTO model_speed (\n--       select model, speed\n--       from pc\n--       where speed >= ALL (\n--           select speed\n--           from pc\n--       )\n--       UNION\n--       select model, speed\n--       from laptop\n--       where speed >= ALL (\n--         select speed\n--         from laptop\n--       )\n-- );\n-- -- main query\n-- SELECT DISTINCT product.maker\n-- FROM product\n-- WHERE product.model IN (\n--   SELECT model\n--   FROM model_speed\n--   WHERE speed >= ALL (\n--     SELECT speed\n--     FROM model_speed\n--   )\n-- );\n-- DROP TABLE model_speed;\n\n-- exercicio 18\n-- SELECT DISTINCT maker\n-- FROM product INNER JOIN (\n--   SELECT model from\n--   pc INNER JOIN (\n--     SELECT speed\n--     FROM pc\n--     GROUP BY speed\n--     HAVING COUNT (speed) >= 3\n--   ) as pc2 USING (speed)\n-- ) as pc3 USING (model);\n\n-- exercicio 19\n-- SELECT maker as prod_count\n-- FROM pc NATURAL JOIN product\n-- GROUP BY maker\n-- HAVING COUNT(*) >= 3;\n\n-- exercicio 20\n-- SELECT AVG(speed)\n-- FROM pc;\n\n-- exercicio 21\n-- SELECT AVG(speed)\n-- FROM pc\n-- WHERE price > 2500;\n\n-- exercicio 22\n-- SELECT AVG(price)\n-- FROM pc NATURAL JOIN product\n-- WHERE maker = 'A'\n\n-- exercicio 23\n-- SELECT AVG(price)\n-- FROM product NATURAL JOIN (\n--   SELECT model,price\n--   FROM pc\n--   UNION\n--   SELECT model,price\n--   FROM laptop\n-- ) as computers\n-- WHERE maker = 'D';\n\n-- exercicio 24\n-- SELECT speed, AVG(price)\n-- FROM pc\n-- GROUP BY speed\n-- ORDER BY speed;\n