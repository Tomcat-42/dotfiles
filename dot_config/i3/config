##bindings para o mpd na parte numerica do teclado
##controles de flow
bindsym KP_Left exec --no-startup-id mpc prev
bindsym KP_Right exec --no-startup-id mpc next
bindsym KP_Begin exec --no-startup-id mpc toggle
##controles de volume n do mpd, mas sim do servidor de audio
bindsym KP_Up exec --no-startup-id "amixer set Master -q 5%+"
bindsym KP_Down exec --no-startup-id "amixer set Master -q 5%-"
bindsym KP_Divide exec --no-startup-id "amixer -q sset Master,0 toggle"
##seek 10s mpd
bindsym KP_Next exec --no-startup-id "mpc seek +5"
bindsym KP_End exec --no-startup-id "mpc seek -5"
##embaralhar a playlist
bindsym KP_Subtract exec --no-startup-id "mpc shuffle"
#volume mpd
bindsym KP_Prior exec --no-startup-id "mpc volume +5"
bindsym KP_Home exec --no-startup-id "mpc volume -5"
##toggle consmume
bindsym KP_Decimal exec --no-startup-id "mpc consume"
##toggle single
bindsym KP_Enter exec --no-startup-id "mpc single"
##toggle aleatório
bindsym KP_Add exec --no-startup-id "mpc random"
##stop mpd
bindsym KP_Insert exec --no-startup-id "mpc stop"
##notificação musica atual
bindsym KP_Delete exec --no-startup-id "/home/pablo951_br/Downloads/scripts/mpdnotify"

