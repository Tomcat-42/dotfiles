<script>
    function scrollToTargetAdjusted(id) {
        var element = document.getElementById(id);
        if(!element) return;
        var headerOffset = 60;
        var elementPosition = element.getBoundingClientRect().top;
        var offsetPosition = elementPosition - headerOffset;

        window.scrollTo({
            top: offsetPosition,
            behavior: "smooth"
        });
    }

    window.addEventListener('load', (event) => {
        var hash = window.location.hash;
        history.pushState("", document.title, window.location.pathname + window.location.search);
        if(hash) {
            scrollToTargetAdjusted(hash.replace('#', ''))
        }
    });

    var bootTracking = function bootIntercom(auth0) {
        var consents = auth0.approvals;
        if (!consents.tracking) {
            return;
        }

        window.auth.getProfile().then(function (profile) {
            if (window.Intercom) {
                return window.Intercom('boot', {
                    app_id: 'sv6pf2jo',
                    user_id: profile['http://arduino.cc/id'],
                    user_hash: consents.identity_hash,
                });
            }
        });
    };

    window.auth = new arduinoHF.Auth0({
        domain: 'login.arduino.cc',
        client_id: '30sUI1PHsoXzwJ9KiDqYIdMmIjsqxiHX',
        redirect_uri: 'https://www.arduino.cc/reference/en/',
        audience: 'https://api.arduino.cc',
        scope: 'openid profile'
    });

    var header = new window.arduinoHF.Header(document.getElementById('header'), window.auth, { env: 'prod', style: 'arduino', search: { tab: 'reference'} });
    new window.arduinoHF.Footer(document.getElementById('footer'), { env: 'prod', style: 'arduino' });
    header.init().then(function(auth0) {
        bootTracking(auth0);
        return auth0;
    });

    
    var arduinoAdvOptions = {
        breakpointDesktop: 768,
        breakpointMobile: 768,
        zoneIdDesktop: 564,
        zoneIdTablet: 563,
        zoneIdMobile: 563
    };
</script>
